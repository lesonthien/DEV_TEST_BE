{
  "name": "nestjs-project-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --entryFile repl",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.ts -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "yarn build && node dist/swagger",
    "migration:generate": "yarn typeorm -- migration:generate",
    "migration:run": "yarn typeorm -- migration:run",
    "migration:revert": "yarn typeorm -- migration:revert",
    "typeorm": "typeorm-ts-node-commonjs",
    "entity:migration": "typeorm-ts-node-commonjs migration:run -d bin/ormconfig.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.2.8",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.6",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/oracledb": "^6.5.3",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.2",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "nestjs-i18n": "^10.4.9",
    "nestjs-pino": "^3.5.0",
    "oracledb": "^5.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.16.1",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-model-generator": "^0.4.6-no-engines"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.22",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/testing": "^10.2.8",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.8",
    "@types/md5": "^2.3.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.18.9",
    "@types/passport": "^1.0.15",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/prompts": "^2.4.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/nestjs-project-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/nestjs-project-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT",
  "lint-staged": {
    "**/*.{ts,js}": "eslint --fix"
  }
}
